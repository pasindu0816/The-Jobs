<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.codewithpasi.jobsconsultancy.controller.UserController</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ConsultantController</servlet-name>
  	<display-name>ConsultantController</display-name>
  	<description></description>
  	<servlet-class>com.codewithpasi.jobsconsultancy.controller.ConsultantController</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>AdminController</servlet-name>
  	<display-name>AdminController</display-name>
  	<description></description>
  	<servlet-class>com.codewithpasi.jobsconsultancy.controller.AdminController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/userlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/userregistration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/consultants</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/getconsultants</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/setappointment</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>ConsultantController</servlet-name>
  	<url-pattern>/getappointment</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>ConsultantController</servlet-name>
   	<url-pattern>/loginConsultant</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>AdminController</servlet-name>
   	<url-pattern>/getadmin</url-pattern>
   </servlet-mapping><servlet-mapping>
   	<servlet-name>AdminController</servlet-name>
   	<url-pattern>/usermanager</url-pattern>
   </servlet-mapping>

</web-app>
